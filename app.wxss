}
.float-right{
  float: right;
}
.c-primary{
  color: var(--primary-color);
}
.c-secondary{
  color: var(--secondary-color);
}
.c-link{
  color: var(--link-color);
}
.ellipsis{
  --w:5em;
  max-width: var(--w);
  text-overflow: ellipsis;
  overflow:hidden;
  white-space:nowrap;
}
.w-full{
  width: 100%;
}
.h-full{
  height: 100%;
}
/* 其他样式 */
.cell-group {
  margin-top: var(--md) !important;
}
.grid-warp{
  background-color: #fff !important;
}
.navigation{
  background-color: var(--primary-color);
}
.tab-class{
  flex:unset !important;
}
.common-bg{
  --bg:var(--primary-color);
  --bg1:#fcf9f1;
  position: fixed;
  width: 100%;
  height: 100%;
  z-index: -1;
  background: linear-gradient(to bottom,var(--bg),var(--bg1));
}
.van-tabs__line{
  background: var(--primary-color) !important;
}